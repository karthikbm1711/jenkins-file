piepline {
    agent any 
    stages {
        stage('parallel') {
            parallel {
                stage('STAGE-1') {
                    when {
                        catchError(buildResult: 'SUCCESS', stageResult: 'SUCCESS')
                    }
                    steps{
                        sh "echo STEGE-2 executes if branch is master"
                    }
                }

                stage('STAGE-2'){
                    steps{
                        sh "echo STEGE-2 executes if branch is master"
                    }
                }
            }    
        }    
    }
}
