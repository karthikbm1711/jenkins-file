pipeline {
    agent any 
      stages {
	      stage('parallel') {
	         parallel {		 
                stage('A') {
                 steps{
                    sh 'echo"Stage A"'
                  }
                }    
         

                stage('B'){
                  steps {
             	     sh 'echo"Stage B"'		
                   }
                }
		      }
		    }
            stage('C'){ 
               steps {
			      sh 'echo"Stage c"'
                }
		    }
    }
}
